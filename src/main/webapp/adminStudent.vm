    </ul>
    <div class="adminContent">
        <h1 class="mt20 mb20">创建学生管理相关</h1>
        <select class="se" onchange="AdminStudentObject.selectChange()">
            <option value="">创建学生生产性实习安全责任书</option>
            <option value="">创建实习手册</option>
            <option value="">创建顶岗实习评价表</option>

        </select>
        <div class="newsBox1">
            <h2 class="mt20 mb20">创建学生生产性实习安全责任书</h2>

            <input type="file" id="file1" name="file">
            <input type="button" value="上传" onclick="AdminStudentObject.fileUpLoad('file1','安全责任书')"/>
        </div>
        <div class="newsBox2">
            <h2 class="mt20 mb20">创建实习手册</h2>

            <input type="file" id="file2" name="file">
            <input type="button" value="上传" onclick="AdminStudentObject.fileUpLoad('file2','实习手册')"/>
        </div>
        <div class="newsBox3">
            <h2 class="mt20 mb20">创建顶岗实习评价表</h2>
                <input type="file" id="file3" name="file">
                <input type="button" value="上传" onclick="AdminStudentObject.fileUpLoad('file3','顶岗实习评价表')"/>
        </div>
    </div>
    <script src="../../js/ajaxfileupload.js"></script>
    <script type="text/javascript" src="../../js/adminBase.js"></script>
    <script type="text/javascript" src="../../js/adminStudent.js"></script>
